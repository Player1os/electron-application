{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Electron: Main",
			"type": "node",
			"request": "launch",
			"protocol": "inspector",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"start"
			]
		},
		{
			"name": "Electron: Renderer",
			"type": "chrome",
			"request": "attach",
			"port": 9223,
			"webRoot": "${workspaceRoot}",
			"timeout": 30000
		}
	],
	"compounds": [
		{
			"name": "Electron: All",
			"configurations": [
				"Electron: Main",
				"Electron: Renderer"
			]
		}
	]
}
